config tinc-net qaulvpn
	option enabled 0

	# name must be unique with only alphanumeric characters and under score
#	option Name qaulUserName

	# connect actively to those servers
#	list ConnectTo qaulHubCH

	option DirectOnly 1
	option Broadcast direct
	option IndirectData 1
	option Mode switch
#	option Port 4223
	option PingInterval 60
	option PingTimeout 10
	option PrivateKeyFile /etc/tinc/qaulvpn/rsa_key.priv
