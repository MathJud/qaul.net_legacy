config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/ar934x_wmac'
	option htmode	HT20
	option country  DE
	option rxldpc	0
	option ldpc	0
	option short_gi_20 0
	option distance '600'

config wifi-iface qaulwifi
	option device      'radio0'
	option encryption  'none'
	option network     'qaulwifi'
	option ssid        'qaul.net'
	option mode        'adhoc'
	option bssid       '02:11:87:88:D6:FF'
	option disabled    0

config wifi-iface lanwifi
	option device      'radio0'
	option network     'lanwifi'
	option mode        'ap'
	option ssid        'qaul.net_AP'
	option encryption  'none'
#	option encryption  'psk2'
#	option key         'bU6D5EYV6WWj'
	option disabled    0
